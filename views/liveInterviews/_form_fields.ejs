<div class="form-group">
  <label for="title">Title</label>
  <input required value="<%= liveInterview.title %>" type="text" name="title" id="title" class="form-control">
</div>
<div class="form-group">
  <label for="videoUrl">Video Url</label>
  <input required value="<%= liveInterview.videoUrl %>" type="text" name="videoUrl" id="videoUrl" class="form-control">
</div>
<div class="form-group">
  <label for="thumbnail">Thumbnail Url</label>
  <input required value="<%= liveInterview.thumbnail %>" type="text" name="thumbnail" id="thumbnail" class="form-control">
</div>
<div class="form-group">
  <label for="description">Description</label>
  <textarea required name="description" id="description" class="form-control">
    <%= liveInterview.description %>
  </textarea>
</div>

<a href="/admin" class="btn btn-secondary">Cancel</a>
<button type="submit" class="btn btn-primary">Save</button>
